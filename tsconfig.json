    "target": "esnext",
    "incremental": true,
    "tsBuildInfoFile": "./tsbuildinfo.json",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "rootDir": "src",
    "outDir": "dist"
  },
  "include": ["./src/**/*.ts"]
}
